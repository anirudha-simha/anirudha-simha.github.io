{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true // Typically, vite.config.ts is not emitted separately by tsc
  },
  "include": ["vite.config.ts"]
}
